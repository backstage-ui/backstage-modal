{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": 0,
    "react/prefer-es6-class": 0,
    "react/prefer-stateless-function": 0,
    "react/require-extension": "off"
  }
}
